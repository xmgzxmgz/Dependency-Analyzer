{
  "name": "frontend-dependency-analyzer",
  "version": "1.0.0",
  "description": "前端组件库依赖关系可视化与冗余代码分析工具",
  "main": "src/index.js",
  "bin": {
    "dep-analyzer": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "test": "mocha tests/**/*.test.js --timeout 10000",
    "test:unit": "mocha tests/unit/**/*.test.js",
    "test:integration": "mocha tests/integration/**/*.test.js --timeout 30000",
    "test:performance": "mocha tests/performance/**/*.test.js --timeout 60000",
    "test:coverage": "nyc npm test",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "build": "echo 'No build step required'",
    "dev": "nodemon bin/cli.js",
    "benchmark": "npm run test:performance"
  },
  "keywords": [
    "dependency",
    "analysis",
    "visualization",
    "frontend",
    "react",
    "vue",
    "ast"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@vue/compiler-sfc": "^3.3.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fs-extra": "^11.1.0",
    "glob": "^10.3.0",
    "ora": "^7.0.1",
    "p-limit": "^3.1.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "chai": "^4.3.7",
    "eslint": "^8.0.0",
    "mocha": "^10.2.0",
    "mock-fs": "^5.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
